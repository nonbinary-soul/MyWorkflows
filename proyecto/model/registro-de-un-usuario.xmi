<?xml version="1.0" encoding="UTF-8"?>
<proyectomm:ProcesoDeNegocio
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:proyectomm="http://www.example.org/proyectomm"
    nombre="Registro de un usuario">
  <bases_de_datos
      nombre="Pizzeria">
    <tablas
        nombre="tabla_clientes">
      <atributos
          xsi:type="proyectomm:PrimaryKey"
          nombre="dni"/>
      <atributos
          nombre="nombre"
          tipo="TEXT"/>
      <atributos
          nombre="direccion"
          tipo="TEXT"/>
      <atributos
          nombre="viveEnCaceres"
          tipo="BOOLEAN"/>
      <atributos
          nombre="cReal"
          tipo="REAL"/>
      <atributos
          nombre="cDate"
          tipo="DATE"/>
      <atributos
          nombre="telefono"
          tipo="TEXT"/>
      <atributos
          nombre="opciones"
          tipo="LIST">
        <opciones
            nombre="item 1"/>
        <opciones
            nombre="item 2"/>
      </atributos>
      <atributos
          nombre="correo_electronico"
          tipo="TEXT"/>
    </tablas>
  </bases_de_datos>
  <actores nombre="Cliente">
    <tareas
        xsi:type="proyectomm:Inicio"
        sucesor="//@actores.0/@tareas.1"/>
    <tareas
        xsi:type="proyectomm:Usuario"
        sucesor="1000"
        predecesor="//@actores.0/@tareas.0"
        nombre="rellenar formulario de registro"
        tabla_accedida="//@bases_de_datos.0/@tablas.0"/>
    <tareas
        xsi:type="proyectomm:Servicio"
        sucesor="20000"
        predecesor="//@actores.0/@tareas.1"
        id="1000"
        nombre="imprimir registro"
        descripcion="imprimir registro"
        tabla_accedida="//@bases_de_datos.0/@tablas.0"/>
    <tareas
        xsi:type="proyectomm:Envio"
        sucesor="10000"
        predecesor="1000"
        id="20000"
        nombre="envio"
        descripcion="envia cosas"
        tabla_accedida="//@bases_de_datos.0/@tablas.0"/>
    <tareas
        xsi:type="proyectomm:Fin"
        predecesor="4000"/>
  </actores>
  <actores nombre="Pizzeria">
    <tareas
        xsi:type="proyectomm:Recepcion"
        sucesor="3000"
        predecesor="20000"
        id="10000"
        nombre="Recepcion"
        descripcion="Recibe cosas"
        tabla_accedida="//@bases_de_datos.0/@tablas.0"/>
    <tareas
        xsi:type="proyectomm:Lectura"
        sucesor="4000"
        predecesor="10000"
        id="3000"
        nombre="lectura"
        descripcion="esta es la descripcion"
        atributo_seleccionado="//@bases_de_datos.0/@tablas.0/@atributos.1"/>
    <tareas
        xsi:type="proyectomm:Borrado"
        sucesor="//@actores.0/@tareas.4"
        predecesor="3000"
        id="4000"
        nombre="tareaBorrado"
        descripcion="este es la descripcion de la tarea"
        atributo_seleccionado="//@bases_de_datos.0/@tablas.0/@atributos.4"/>
  </actores>
</proyectomm:ProcesoDeNegocio>
