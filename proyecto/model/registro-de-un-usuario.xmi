<?xml version="1.0" encoding="UTF-8"?>
<proyectomm:ProcesoDeNegocio
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:proyectomm="http://www.example.org/proyectomm"
    nombre="Registro de un usuario">
  <bases_de_datos
      nombre="Pizzeria">
    <tablas
        nombre="tabla_clientes">
      <atributos
          xsi:type="proyectomm:PrimaryKey"
          nombre="dni"/>
      <atributos
          nombre="nombre"
          tipo="TEXT"/>
      <atributos
          nombre="direccion"
          tipo="TEXT"/>
      <atributos
          nombre="jaja"
          tipo="BOOLEAN"
          valor="0"/>
      <atributos
          nombre="dafafds"
          tipo="REAL"/>
      <atributos
          nombre="asdffdsfds"
          tipo="DATE"/>
      <atributos
          nombre="telefono"
          tipo="TEXT"/>
      <atributos
          nombre="opciones"
          tipo="LIST"
          valor="item1, item2, item3"/>
      <atributos
          nombre="correo_electronico"
          tipo="TEXT"/>
    </tablas>
  </bases_de_datos>
  <actores nombre="Cliente">
    <tareas
        xsi:type="proyectomm:Inicio"
        sucesor="//@actores.0/@tareas.1"/>
    <tareas
        xsi:type="proyectomm:Usuario"
        sucesor="//@actores.0/@tareas.2"
        predecesor="//@actores.0/@tareas.0"
        nombre="rellenar formulario de registro"
        tabla_accedida="//@bases_de_datos.0/@tablas.0"/>
    <tareas
        xsi:type="proyectomm:Fin"
        predecesor="//@actores.0/@tareas.1"/>
  </actores>
</proyectomm:ProcesoDeNegocio>
